packadd previm "マークダウンのプレビュー表示{
  let g:previm_open_cmd='start'
"}

packadd vim-auto-save "自動保存{
  let g:auto_save = 1  " enable AutoSave on Vim startup
  let g:auto_save_silent = 1  " do not display the auto-save notification
"}

